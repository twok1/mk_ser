    "def split_by_two(what=str|int) -> list:\n",
    "    what = str(what)[::-1]\n",
    "    return [i[::-1] for i in [what[i:i+2] for i in range(0, len(what), 2)][::-1]]\n",
    "\n",
    "split_by_two(1)"
